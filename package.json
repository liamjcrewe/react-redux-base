{
  "name": "myreactapp",
  "version": "1.0.0",
  "description": "",
  "main": "build.js",
  "scripts": {
    "build": "NODE_ENV=production browserify src/main.js -t babelify -t strictify -t envify -g uglifyify | uglifyjs -c > public/build.js",
    "watch": "watchify -d src/main.js -t babelify -t strictify -o '> public/build.js && standard | snazzy' -v",
    "test": "standard | snazzy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "snazzy": "^7.0.0",
    "standard": "^10.0.3",
    "strictify": "^0.2.0",
    "watchify": "^3.10.0"
  },
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^13.0.1",
    "envify": "^4.1.0",
    "lodash.merge": "^4.6.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "uglify-js": "^3.3.10",
    "uglifyify": "^4.0.5"
  }
}
